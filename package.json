{
  "name": "bitpanda-portfolio-viewer",
  "version": "1.0.0",
  "description": "Eine Web-Anwendung zur Anzeige Ihres Bitpanda-Portfolios",
  "main": "backend/app.py",
  "scripts": {
    "install": "pip install -r requirements.txt && cd frontend && npm install",
    "start": "python backend/app.py",
    "dev": "concurrently \"python backend/app.py\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "bitpanda",
    "portfolio",
    "crypto",
    "react",
    "flask",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/bitpanda-portfolio-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/username/bitpanda-portfolio-viewer/issues"
  },
  "homepage": "https://github.com/username/bitpanda-portfolio-viewer#readme"
}
