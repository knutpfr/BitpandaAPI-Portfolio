{
  "name": "bitpanda-portfolio-viewer",
  "version": "2.0.0",
  "description": "Eine moderne Web-Anwendung zur Visualisierung Ihres Bitpanda-Portfolios mit interaktiven Charts",
  "main": "backend/app.py",
  "scripts": {
    "install": "pip install -r requirements.txt && cd frontend && npm install",
    "start": "python backend/app.py",
    "dev": "concurrently \"python backend/app.py\" \"cd frontend && npm run dev\"",
    "build": "cd frontend && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "bitpanda",
    "portfolio",
    "crypto",
    "react",
    "flask",
    "api",
    "charts",
    "dark-theme",
    "visualization"
  ],
  "author": "Knut",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knutpfr/BitpandaAPI-Portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/knutpfr/BitpandaAPI-Portfolio/issues"
  },
  "homepage": "https://github.com/knutpfr/BitpandaAPI-Portfolio#readme"
}
